    "cmd_identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z?!:\\.+*=\\/][a-zA-Z0-9?!:\\.+*=\\/]*"
    },
    "arg_identifier": {
      "type": "PATTERN",
      "value": "[^\\r\\n#@|>; ]+"
    },
    "number": {
      "type": "PATTERN",
      "value": "[1-9]+[0-9]*"
    },
